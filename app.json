{
  "expo": {
    "name": "PumpfoilMap",
    "slug": "pumpfoilmap",
    "scheme": "pumpfoilmap",
    "version": "0.1.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "logos/*"
    ],
    "splash": {
      "backgroundColor": "#0b3d91",
      "resizeMode": "contain",
      "image": "./logos/logo_128_rgba.png"
    },
    "ios": { "supportsTablet": true },
    "android": {
      "package": "com.pumpfoilmap.app",
      "splash": {
        "backgroundColor": "#0b3d91",
        "resizeMode": "contain",
        "image": "./logos/logo_128_rgba.png"
      }
    },
    "web": { "bundler": "metro", "favicon": "./logos/favicon.png" }
  }
}
