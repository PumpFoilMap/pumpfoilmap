{
  "name": "pumpfoilmap-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "npm-run-all -p dyn:start offline",
    "offline": "serverless offline --stage dev",
    "dyn:start": "serverless dynamodb start --stage dev",
    "dyn:stop": "serverless dynamodb stop",
    "build": "serverless package",
    "test": "jest --passWithNoTests",
    "test:e2e": "jest --config jest.e2e.config.cjs --passWithNoTests",
    "e2e:run": "node scripts/e2e-runner.cjs",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/lib-dynamodb": "^3.654.0",
    "svg-captcha": "^1.4.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.155",
    "@types/jest": "^29.5.12",
    "axios": "^1.12.2",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "serverless": "^3.39.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.54.4",
    "serverless-offline": "^13.8.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
